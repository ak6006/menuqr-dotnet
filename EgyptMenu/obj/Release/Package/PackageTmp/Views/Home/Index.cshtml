@using EgyptMenu.Views.Shared.Resources
@{
    Layout = null;
    if (!Request.QueryString["lang"].IsEmpty())
    {
        Culture = UICulture = Request.QueryString["lang"];
        HttpContext.Current.Session.Add("lang", Request.QueryString["lang"]);
    }
    else
    {
        if (HttpContext.Current.Session["lang"] != null)
        {
            Culture = UICulture = HttpContext.Current.Session["lang"].ToString();
            HttpContext.Current.Session.Add("lang", HttpContext.Current.Session["lang"].ToString());
        }
        else
        {
            Culture = UICulture = "en";
            HttpContext.Current.Session.Add("lang", "en");
        }
    }
}
